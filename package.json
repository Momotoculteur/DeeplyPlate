{
  "name": "DeeplyPlate",
  "version": "0.0.0",
  "author": "Bastien MAURICE <bastien_maurice@hotmail.com>",
  "description": "Générateur aléatoire de plaque d'immatriculation. Possibilités d'incruster dans des photos avec divers bruits visuels, avec génération de bounding boxes et exportation sous CSV.",
  "main": "main.js",
  "scripts": {
    "backend-serve": "ttsc --project tsconfig.backend.json --watch",
    "backend-build": "ttsc --project tsconfig.backend.json",
    "frontend-build": "ng build --prod",
    "frontend-serve": "ng serve",
    "electron-build": "electron-builder --config electron-builder.json",
    "electron-serve": "electron . --dev",
    "all-build": "npm run angular-build && npm run backend-build && npm run electron-build --config electron-builder.json",
    "test": "jest --config jest.config.json --detectOpenHandles --runInBand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~9.1.1",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cdk": "^9.2.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/language-service": "~9.1.1",
    "@types/node": "^12.11.1",
    "@zerollup/ts-transform-paths": "^1.7.17",
    "canvas": "^2.6.1",
    "codelyzer": "^5.1.2",
    "electron": "^8.2.4",
    "electron-builder": "^22.5.1",
    "electron-reload": "^1.5.0",
    "electron-store": "^5.1.1",
    "jest": "^25.5.0",
    "material-design-icons-iconfont": "^5.0.1",
    "ngx-electron": "^2.2.0",
    "spectron": "^10.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "ttypescript": "^1.5.10",
    "typescript": "~3.8.3"
  }
}
